/ 점수에 따라 다른 기능을 실행하는 함수
 void _handleRating(double value) {
   if (value == -0.2) {
     HapticFeedback.lightImpact(); // 강한 햅틱

   } else if (value == -0.1) {
     HapticFeedback.lightImpact(); // 중간 햅틱

   } else if (value == 0.0) {
     HapticFeedback.lightImpact(); // 가벼운 햅틱

   } else if (value == 0.1) {
     HapticFeedback.lightImpact(); // 중간 햅틱

   } else if (value == 0.2) {
     HapticFeedback.lightImpact(); // 강한 햅틱
   }
 }
